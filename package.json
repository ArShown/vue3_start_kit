{
  "name": "vue3_start_kit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --write **/*.{vue,js,css}"
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{js,vue,css}": "prettier --write",
    "*.{js,vue}": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@vue/composition-api": "1.0.0-beta.18",
    "@vuelidate/core": "2.0.0-alpha.7",
    "@vuelidate/validators": "2.0.0-alpha.5",
    "autoprefixer": "^9",
    "axios": "0.20.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "core-js": "3.6.5",
    "date-fns": "2.16.1",
    "mockjs": "1.1.0",
    "perfect-scrollbar": "1.5.0",
    "postcss": "^7",
    "ramda": "0.27.1",
    "store2": "2.12.0",
    "sweetalert2": "10.10.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vue": "3.0.2",
    "vue-router": "4.0.0-rc.1",
    "vue-toastification": "2.0.0-beta.7",
    "vue3-datepicker": "0.1.1",
    "vuex": "4.0.0-beta.4",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/compiler-sfc": "3.0.2",
    "babel-eslint": "10.1.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.13.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "7.1.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "prettier": "2.1.2",
    "sass": "1.29.0",
    "sass-loader": "10.0.5"
  }
}
