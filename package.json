{
  "name": "vue3_start_kit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "serve": "vite serve",
    "build": "vite build",
    "build:dev": "vite build --mode development"
  },
  "lint-staged": {
    "*.md": "prettier --write",
